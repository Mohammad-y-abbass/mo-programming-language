{
  "name": "MohLang",
  "displayName": ".moh",
  "description": "Syntax highlighting support for MohLang files (.moh) in Visual Studio Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "moh",
      "aliases": ["moh", "moh"],
      "extensions": [".moh"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "moh",
      "scopeName": "source.moh",
      "path": "./syntaxes/moh.tmLanguage.json"
    }]
  }
}
